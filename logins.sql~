drop database if exists logins  ; 


create database logins ;
use logins ;
create table login ( 
login varchar(8) primary key ,
clave varchar(8) not null ,
nombre varchar(128) not null );

insert into login values ('usuario','usuario','nombre usuario'),('joaquin','joaquin','nombre joaquin');

select * from login where login like 'user' ;

select * from login where login like 'joaquin' ;